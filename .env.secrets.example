# Bonito Secrets Template
# Copy this file to .env.secrets and fill in your actual values
# NOTE: These secrets should be stored in HashiCorp Vault for production

# Notion Integration
NOTION_API_KEY=ntn_your_notion_api_key_here
NOTION_PAGE_ID=your-notion-page-id-here
NOTION_CHANGELOG_ID=your-notion-changelog-id-here

# NextAuth Secret (for frontend authentication)
NEXTAUTH_SECRET=your-nextauth-secret-here

# Environment (development/production)
ENVIRONMENT=development